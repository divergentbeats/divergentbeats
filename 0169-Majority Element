class Solution {
public:
    int majorityElement(vector<int>& nums) {
        int n= nums.size();
        int max=0, count=0;
        unordered_map<int,int> mp;

        for(int i=0;i<n;i++)
        {
            mp[nums[i]]++; 
            if(mp[nums[i]]>count)    //Compare with the frequency
            {
                count= mp[nums[i]];
                max=nums[i];
            }
        }
        return max;
        
    }
};
